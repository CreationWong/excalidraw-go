{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "excalidraw-go",
  "version": "0.2.0",
  "identifier": "com.0d000721.excalidraw-go",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Excalidraw Go",
        "width": 1030,
        "height": 820
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico",
      "icons/icon-32x32.ico",
      "icons/icon-128x128.ico",
      "icons/icon-32x32.png",
      "icons/icon.png"
    ]
  }
}
